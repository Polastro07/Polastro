import lista_gastos 
gastos_joao = [300, 500, 200, 800]
gastos_pedro = [200, 400, 500, 700]

import calculo
total_joao = 0
total_pedro = 0 
for gastos in gastos_joao: 
    total_joao += gastos
    for gastos in gastos_pedro: 
        total_pedro += gastos 

if total_joao > total_pedro:
    print("João gastou mais durante o mês do que Pedro")
elif total_pedro > total_joao:
    print("Pedro gastou mais durante o mês do que João")
else:
    print("João e Pedro a mesma quantidade durante o mês")
